@model IEnumerable<Horario.Domain.Entities.Cita>

@{
    ViewBag.Title = "Panel de Citas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="panel panel-default">
    <div class="panel-heading">
        <h3>Citas</h3>
    </div>

    <div class="panel-body">
        <table class="table table-striped table-condensed table-bordered">
            <tr>
                <th class="text-right">Cita</th>
                <th class="text-right">Dica</th>
                <th>Persona</th>
            </tr>
            @foreach (var cita in Model)
            {
                <tr>
                    <td class="text-right">@cita.Folio</td>
                    <td class="text-right">@cita.Nomina</td>
                    <td>@cita.Correo</td>
                   
                   </tr>
            }
        </table>
    </div>

    <div class="panel-footer">
        @Html.ActionLink("Crear Cita", "Create", null, new { @class = "btn btn-default"})
    </div>
</div>
