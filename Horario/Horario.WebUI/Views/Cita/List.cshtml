@model Horario.Domain.Concrete.EFCitaRepository

@{
    ViewBag.Title = "Lo que hizo vane";
}


@foreach (var cita in Model.Citas)
{
    <div class="well">
        <h3>
            <strong>@cita.Folio</strong>
            <span class="pull-right label label-primary">Algo</span>
        </h3>
        <span class="lead"></span>
        <div>
            <div class="row">
                <div class="col-lg-2"><center><a style="color:black" href="#" class="btn btn-primary btn-lg" role="button">Ver Horario</a></center></div>
                <div class="col-lg-1"><center><a style="color:black" href="/Cita/Index" class="btn btn-primary btn-lg" role="button">Ver Citas</a></center></div>
            </div>
            <div><br /></div>

        </div>
    </div>
}

