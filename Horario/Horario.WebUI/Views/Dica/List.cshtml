@model Horario.Domain.Concrete.EFDicaRepository

@{
    ViewBag.Title = "Directores de Carrera";
}

@foreach (var dica in Model.Dicas)
{
    <div class="well">
        <h3>
            <strong>@dica.Nomina</strong>
            <span class="pull-right label label-primary">@dica.Fname@dica.Lname</span>
        </h3>
        <span class="lead">@dica.email</span>
        <div>
            <div class="row">
                <div class="col-lg-2"><center><a style="color:black" href="#" class="btn btn-primary btn-lg" role="button">Ver Horario</a></center></div>
                <div class="col-lg-1"><center><a style="color:black" href="/Cita/Index" class="btn btn-primary btn-lg" role="button">Ver Citas</a></center></div>
                <div class="col-lg-2"><center><a style="color:black" href="/Cita/Edit" class="btn btn-primary btn-lg" role="button">Crear Citas</a></center></div>
            </div>
            <div><br /></div>      
            
        </div>
    </div>
}




